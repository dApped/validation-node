  "source": "pragma solidity 0.4.24;\n\n\ncontract EventRegistry {\n    address[] verityEvents;\n\n    event NewVerityEvent(address eventAddress);\n\n    function registerEvent() public {\n        verityEvents.push(msg.sender);\n        emit NewVerityEvent(msg.sender);\n    }\n}",
